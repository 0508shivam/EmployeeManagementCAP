{
	"list1": {
		"sap.app": {
			"id": "sample.CardsLayout.model.list",
			"type": "card"
		},
		"sap.card": {
			"configuration": {
				"filters": {
					"searchQuery": {
						"type": "Search",
						"label": "name",
						"placeholder": "Enter a position"
					}
				},
				"data": {
					"request": {
							"$filter": "contains(name, '{filters>/searchQuery/value}')",
							"$count": true
						}
					}
				},	
			"type": "List",
			"header": {
				"title": "Need Master List",
				"status": {
					"text": "3 of 4"
				}
			},
			"footer": {
				"paginator": {
					"pageSize": 3
				}
			},
			"content": {
				"data": {
					"json": [
                        {
							"name": "Sales Manager",
							"icon": "sap-icon://add-employee",
							"description": "Houston Texas, USA",
							"info": "Lead Case",
							"infoState": "Information"
						},
						{
							"name": "Project Manager",
							"icon": "sap-icon://add-employee",
							"description": "Houston, Texas, USA",
							"info": "Final Reviewed",
                            "infoState": "Success"
						},
						{
							"name": "Product Specialist",
							"icon": "sap-icon://add-employee",
							"description": "Houston Texas, USA",
							"info": "In Progress",
							"infoState": "Information"
						},
						{
							"name": "Employee Central Consultant",
							"icon": "sap-icon://add-employee",
							"description": "Hyderabad, Telangana, India",
							"info": "Rejected",
							"infoState": "Error"
						}
					]
				},
				"item": {
					"icon": {
						"src": "{icon}"
					},
					"title": {
						"value": "{name}"
					},
					"description": {
						"value": "{description}"
					},
					"info": {
						"value": "{info}",
						"state": "{infoState}"
					},
					"actions": [
						{
						  "type": "Navigation",
						  "parameters": {
							"url": "#/needsObjectPage/{name}",
							"target": "_self"
						  }
						}
					  ]	
				}	
			}
		}
	},
